			</timer>
			<!-- Gestor de sockets TCP para un intervalo de puertos -->
			<ftp>
				<!-- Fábrica de clientes de FTP -->
				<FTPClientFactory>
					<!-- Implementación por defecto -->
					<default>isf.util.ftp.FTPClientImpl</default>
				</FTPClientFactory>
				<!-- Manejador ABSTRACTO de transmisión de DATOS para una conexión FTP -->
				<FTPClientDataTransfer>
					<!-- Tiempo máximo de espera para las operaciones I/O (milisegundos) -->
					<so_timeout>20000</so_timeout>
					<!-- Tiempo máximo de espera para el cierre de sockets (milisegundos) -->
					<so_linger>1000</so_linger>
				</FTPClientDataTransfer>
				<!-- Intérprete de protocolo de control para una conexión FTP -->
				<FTPClientProtocolInterpreter>
					<!-- Tiempo máximo de espera para las operaciones I/O (milisegundos) -->
					<so_timeout>10000</so_timeout>
					<!-- Tiempo máximo de espera para el cierre de sockets (milisegundos) -->
					<so_linger>1000</so_linger>
				</FTPClientProtocolInterpreter>
				<!-- Implementación básica de un cliente FTP -->
				<FTPClientImpl>
					<!-- Charset para la conversión bytes -> chars -->
					<charset_encoding>ISO-8859-15</charset_encoding>
					<!-- Deserializador por defecto para los listados completos de ficheros -->
					<deserializer_default>isf.util.ftp.FileInfoDeserializer_Unix</deserializer_default>
				</FTPClientImpl>
				<!-- Deserializador de información-ficheros para FileSystems estilo Unix -->
				<FileInfoDeserializer_Unix>
					<!-- Formato por defecto de cada línea serializada -->
					<format_default>{attrs}{ignore}{ignore}{ignore}{size}{last_date}{name}</format_default>
					<!-- Idioma por defecto para los formatos de fecha admitidos -->
					<locale_language>en</locale_language>
					<!-- Formatos de fecha admitidos -->
					<date_formats>
						<date_format1>MMM dd hh:mm</date_format1>
						<date_format2>MMM dd  yyyy</date_format2>
					</date_formats>
				</FileInfoDeserializer_Unix>
			</ftp>
