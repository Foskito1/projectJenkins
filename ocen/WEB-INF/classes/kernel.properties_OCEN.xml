			<!--Propiedades del Pool de conexiones JDBC-->
			<Pool>
				<!--Clase de implementación del pool: isf.db.BasicPool (primitivo) o isf.db.AdvancedPool (revisado)-->
				<implementation_class>isf.db.AdvancedPool</implementation_class>
				<!--Indicador de comportamiento al liberar abruptamente: (0:ninguno | 1:liberar & abortar thread usuario | 2: liberar y destruir conexión)-->
				<abrupt_behaviour>2</abrupt_behaviour>
				<!--Tiempo de recuperación ante errores JDBC (ms)-->
				<error_recovery_time>2000</error_recovery_time>
				<!--Clase de generación de secuencias-->
				<sequence_generator>isf.db.SequenceGeneratorTableBased</sequence_generator>
				<!--Mínimo lapso de tiempo admisible entre instantes de aviso de inactividade (en ms)-->
				<min_activity_time_step>1000</min_activity_time_step>
				<!--Indicador de comportamiento al finalizar la JVM: false: no hacer nada; true: liberar todas las conexiones por la fuerza-->
				<release_on_shutdown>true</release_on_shutdown>
				<!--Indicador de forma de cierre de las conexiones: true: asíncrono; false: síncrono-->
				<asynchronous_close>true</asynchronous_close>
				<!--Listeners asociados a los drivers-->
				<driver_listeners>
					<oracle.jdbc.driver.OracleDriver>oracle.jdbc.driver.OracleDriver</oracle.jdbc.driver.OracleDriver>
					<org.postgresql.Driver>org.postgresql.Driver</org.postgresql.Driver>
				</driver_listeners>
				<!--Listeners globales-->
				<global_listeners>
					<listener.1>isf.db.ConnectionMonitor</listener.1>
				</global_listeners>
				<!--Obligación de envío de información de traceo al motor BD Oracle-->
				<WRITE_CLIENT_INFO>true</WRITE_CLIENT_INFO>
				<!--Período de sondeo del estado de cada conexión-->
				<connection_verification_interval>180000000</connection_verification_interval>
				<autoCommit>false</autoCommit>
			</Pool>
