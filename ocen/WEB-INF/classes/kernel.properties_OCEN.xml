		<util>
			<!--Gestor de sockets TCP para un intervalo de puertos-->
			<ftp>
				<!--Fábrica de clientes de FTP-->
				<FTPClientFactory>
					<!--Implementación por defecto-->
					<default>isf.util.ftp.FTPClientImpl</default>
				</FTPClientFactory>
				<!--Manejador ABSTRACTO de transmisión de DATOS para una conexión FTP-->
				<FTPClientDataTransfer>
					<!--Tiempo máximo de espera para las operaciones I/O (milisegundos)-->
					<so_timeout>20000</so_timeout>
					<!--Tiempo máximo de espera para el cierre de sockets (milisegundos)-->
					<so_linger>1000</so_linger>
				</FTPClientDataTransfer>
				<!--Intérprete de protocolo de control para una conexión FTP-->
				<FTPClientProtocolInterpreter>
					<!--Tiempo máximo de espera para las operaciones I/O (milisegundos)-->
					<so_timeout>10000</so_timeout>
					<!--Tiempo máximo de espera para el cierre de sockets (milisegundos)-->
					<so_linger>1000</so_linger>
				</FTPClientProtocolInterpreter>
				<!--Implementación básica de un cliente FTP-->
				<FTPClientImpl>
					<!--Charset para la conversión bytes -> chars-->
					<charset_encoding>ISO-8859-1</charset_encoding>
					<!--Deserializador por defecto para los listados completos de ficheros-->
					<deserializer_default>isf.util.ftp.FileInfoDeserializer_Unix</deserializer_default>
				</FTPClientImpl>
				<!--Deserializador de información-ficheros para FileSystems estilo Unix-->
				<FileInfoDeserializer_Unix>
					<!--Formato por defecto de cada línea serializada-->
					<format_default>{attrs}{ignore}{ignore}{ignore}{size}{last_date}{name}</format_default>
					<!--Idioma por defecto para los formatos de fecha admitidos-->
					<locale_language>en</locale_language>
					<!--Formatos de fecha admitidos-->
					<date_formats>
						<date_format1>MMM dd hh:mm</date_format1>
						<date_format2>MMM dd  yyyy</date_format2>
					</date_formats>
				</FileInfoDeserializer_Unix>
			</ftp>
			<!--Cliente de servicios remotos ARQv10 con parámetros por defecto-->
			<!--Encriptacion-->
			<crypt>
				<KeyStoreMediator>
					<!--Password elegida para el almacen de claves-->
					<key_store_password>VincentyJules</key_store_password>
					<!--Localizacion del fichero que soporta el almacen de claves-->
					<key_store_file_path>file:/soflib00/evol/ocen/webapps/WEB-INF/classes/properties/crypt/.keystore</key_store_file_path>
				</KeyStoreMediator>
			</crypt>
			<!--Logs-->
			<log>
				<providers>
					<!-- Proveedores registrados. -->
					<console>isf.util.log.ConsoleLog</console>
					<!--<console>isf.util.log.ConsoleLog</console>-->
				</providers>
				<Log>
					<!-- Se han definido 6 niveles de trazas : -->
					<!-- INFO_LEVEL(1) Solo trazas sobre la ejecución -->
					<!-- CRITICAL_ERROR_LEVEL (2) Sólo errores críticos y trazas de información. -->
					<!-- ERROR_LEVEL(3). Errores normales y todos los anteriores niveles. -->
					<!-- WARNING_LEVEL(4). Avisos y todos los anteriores niveles. -->
					<!-- DETAIL_LEVEL(5). Trazas de detalle de la ejecucion y todos los anteriores. -->
					<!-- TRACE_LEVEL(6). Trazas de desarrollo y todos los anteriores. -->
					<!-- DEACTIVATE_LEVEL (0) Trazas desactivadas. -->
					<!-- Máscara de niveles reportables (criterio más prioritario) -->
					<reportable_level_codes>1,2,3,4,5,6</reportable_level_codes>
					<!-- Máximo nivel reportable (criterio menos prioritario) -->
					<max_trace_level>6</max_trace_level>
					<resolver_implementation_class>isf.util.log.LogActivityResolver</resolver_implementation_class>
				</Log>
				<LogActivityResolver>
					<!-- Numero de hilos que se encargan de dejar las trazas. -->
					<num_threads>6</num_threads>
					<!-- Tiempo de muestreo de actividades pendientes (en milisegundos) -->
					<poll_time>1000</poll_time>
				</LogActivityResolver>
				<LogActivityResolverOptimized> 
					<!-- Numero de hilos que se encargan de dejar las trazas. --> 
					<num_threads>10</num_threads> 
				</LogActivityResolverOptimized>
			</log>
			<!--Monitoring Threads entries factory-->
			<tms>
				<ThreadMonitoringService>
					<!--Monitoring entries factory-->
					<entry_factory>isf.util.tms.ServletCallInfoFactory</entry_factory>
					<!--Monitoring device-->
					<monitoring_device>isf.util.tms.LogDevice</monitoring_device>
					<!--Max number of concurrent entries-->
					<max_entries>2000</max_entries>
					<!--Parameters for servelt call monitoring-->
					<monitorize_servlets>false</monitorize_servlets>
					<call_connections_info>false</call_connections_info>
					<call_servlet_name>true</call_servlet_name>
					<call_service_name>true</call_service_name>
					<call_session>true</call_session>
					<call_begin_date>true</call_begin_date>
					<call_end_date>true</call_end_date>
					<call_input>true</call_input>
					<call_output>false</call_output>
					<call_is_authorized>false</call_is_authorized>
					<call_client_IP_address>true</call_client_IP_address>
					<call_extra_info>true</call_extra_info>
					<!--Parameters for connection monitoring-->
					<conn_profile_id>false</conn_profile_id>
					<conn_assigned_to>false</conn_assigned_to>
					<conn_open_date>false</conn_open_date>
					<conn_close_date>false</conn_close_date>
					<conn_extra_info>false</conn_extra_info>
					<conn_open_date_ms>false</conn_open_date_ms>
					<conn_close_date_ms>false</conn_close_date_ms>
				</ThreadMonitoringService>
